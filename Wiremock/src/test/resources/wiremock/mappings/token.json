{
	"request": 
	{
		"urlPath": "/token",
		"method": "POST",
		"queryParameters": 
		{
			"grant_type": 
			{
				"equalTo": "client_credentials"
			}
		},
		"headers": 
		{
			"Authorization": {
				"equalTo": "Basic MTIzNDo0MzIx"
			}
		}
	},
	"response": 
	{
		"status": 200,
		"bodyFileName": "tokenCredentialsResponse.json",
		"headers": 
		{
			"Access-Control-Allow-Methods": "POST",
			"Connection": "Keep-Alive",
			"Access-Control-Allow-Headers": "content-type",
			"Content-Type": "application/json"
		}
	}
}